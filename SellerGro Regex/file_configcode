                                self.fba1=data["fba"]
                                self.total_sellers1=data["total_sellers"]
                                self.binding1=data["binding"]
                                self.domain1=data["domain"]
                                self.category1=data["category"]
                                self.sub_cat_sales_rank1=data["sub_cat_sales_rank"]
