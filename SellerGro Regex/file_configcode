def file_config(self):
                with open('/home/sai/prod_reco/config/file_config.json') as config_file:
                                data=json.load(config_file)
                                self.author1=data["author"]
                                self.title1=data["title"]
                                self.isbn1=data["isbn_13"]
                                self.publisher1=data["publisher"]
